def LargestFunc(list1, N):
	new_list = []
	for i in range(0, N):
		max1 = 0
		for j in range(len(list1)):
			if list1[j] > max1:
				max1 = list1[j];
		list1.remove(max1);
		new_list.append(max1)
	print("Largest numbers = ",new_list)
my_list = [12, 61, 41, 85, 40, 13, 77, 65, 100]
N = 4
LargestFunc(my_list, N)
